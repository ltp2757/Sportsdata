# Navigate to your project directory
cd /path/to/your/project

# Initialize a new Git repository
git init

# Create the Python script
echo 'print("Hi, World!")' > hi_world.py

# Add the script to the repository
git add hi_world.py

# Commit the script
git commit -m "Add hi_world.py script"

# Link the local repository to GitHub
git remote add origin https://github.com/ltp2757/Sportsdata/edit/main/GPStracking

# Push the script to GitHub
git push -u origin master
